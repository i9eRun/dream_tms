                                                            "",
                                                            "    // 서버 호출",
                                                            "    Ext.Ajax.request({",
                                                            "        url: TmsLabel.util.Common.BASE_URL + '/tmslabel/save',",
                                                            "        method: 'POST',",
                                                            "        jsonData: data,",
                                                            "        success: function () {",
                                                            "            Ext.Msg.alert('성공', '저장이 완료되었습니다.');",
                                                            "            store.commitChanges();",
                                                            "            store.reload();",
                                                            "        },",
                                                            "        failure: function () {",
                                                            "            Ext.Msg.alert('실패', '저장 중 오류가 발생했습니다.');",
                                                            "        }",
                                                            "    });",
